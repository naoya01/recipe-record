<div class="l-main_container">

  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Meal description</th>
        <th>Date</th>
        <th>Mealtime</th>
        <th>start_date</th>
        <th>end_date</th>
        <th colspan="3"></th>
      </tr>
    </thead>
  
    <tbody>
      <% @meals.each do |meal| %>
        <tr>
          <td><%= meal.title %></td>
          <td><%= meal.meal_description %></td>
          <td><%= meal.date %></td>
          <td><%= meal.mealtime %></td>
          <td><%= meal.start_date %></td>
          <td><%= meal.end_date %></td>
  
          <td><%= link_to 'Show', meal %></td>
          <td><%= link_to 'Edit', edit_meal_path(meal) %></td>
          <td><%= link_to 'Destroy', meal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <%= link_to 'New Meal', new_meal_path %>
  <br>
  <div id='calendar'></div>

</div>